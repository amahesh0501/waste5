<div class="navbar role="navigation">
     <div class="container-fluid">
       <div class="navbar-header">

         <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
           <span class="sr-only">Toggle navigation</span>
           <span class="icon-bar"></span>
           <span class="icon-bar"></span>
           <span class="icon-bar"></span>
         </button>
         <img class="logo" src="http://i.imgur.com/mUvk8TD.png" />
       </div>
       <div class="navbar-collapse collapse">
         <ul class="nav navbar-nav navbar-right">
          <% if current_user %>
            <li class="nav-link"><%= link_to "#{current_user.name}'s Dashboard", root_path  %></li>
            <li class="nav-link"><%= link_to "Log Out", destroy_user_session_path  %></li>
          <% else %>
            <li class="nav-link"><%= link_to "Sign In", user_omniauth_authorize_path(:facebook) unless current_user %></li>
            <li class="nav-link"><%= link_to "Sign Up", user_omniauth_authorize_path(:facebook) unless current_user %></li>
          <% end %>


         </ul>



        <%= form_tag search_path, method: 'get', class: "search-form" do %>
        <p>
          <img class="search-icon" src="http://png-4.findicons.com/files/icons/1389/g5_system/32/toolbar_find.png"/>
          <%= text_field_tag :query, params[:query], id: 'search_from' ,class: 'tags_with_autocomplete', :data => {autocompleteurl: autocomplete_tag_name_tags_path, placeholder: "Search standup..."} %>
        </p>
        <% end %>

       </div>
     </div>
   </div>