<script src="http://www.youtube.com/iframe_api"></script>

<div class="show_video_page">
  <center>
  <iframe id="explore_js" width="100%" height="700" src="https://www.youtube.com/embed/<%=@video_id%>?enablejsapi=1&autoplay=1&autohide=1&showinfo=0" frameborder="0" class="explore_embed" allowfullscreen></iframe>
  </center>
  <div class="row video_information">
    <div class="col-md-1"></div>
    <div class="col-md-8">
      <h3><%= @post.title %></h3>
      <% if @post.date %>
        <p>Posted on <%= @post.date.strftime("%m/%d/%Y") %></p>
      <% end %>
      <p><%= link_to "Skip and report as not standup", {:controller => "reports", :action => "report", :post_to_report => @post.id }%></p>
    </div>
    <div class="col-md-2">
      <%= button_to "Next Video", comedian_path(@comedian, :autoplay => 1), :method => :get, class: "next-video-button-below" %>


    </div>
    <div class="col-md-1"></div>
  </div>

</div>